{
  "name": "<%= projectName %>",
  "version": "0.0.1",
  "description": "Description goes here",
  "main": "index.js",
  "scripts": {
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha -- --recursive",
    "db": "npm run db:drop && npm run db:migrate",
    "db:drop": "sequelize db:migrate:undo:all",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:seed": "sequelize db:seed:all",
    "db:seed:undo": "sequelize db:seed:undo",
    "lint": "eslint .",
    "migration:create": "sequelize migration:create",
    "model": "sequelize model:create",
    "prepush": "NODE_ENV=test npm run lint && npm test",
    "pretest": "npm run lint",
    "start": "node .",
    "start:dev": "npm run db && supervisor .",
    "test": "NODE_ENV=test mocha --recursive",
    "watch": "chokidar 'lib/**/*.js' 'lib/**/*.json' 'test/**/*.js' 'test/**/*.json' -c 'npm test' --initial"
  },
  "author": "",
  "license": "UNLICENSED",
  "repository": "",
  "dependencies": {
    "jsonwebtoken": "^5.5.4",
    "parch": "1.0.0",
    "pg": "^4.5.1",
    "pg-connection-string": "^0.1.3",
    "pg-hstore": "^2.3.2",
    "pg-native": "^1.10.0",
    "sequelize": "^3.19.3",
    "sequelize-cli": "^2.3.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chokidar-cli": "^1.2.0",
    "conventional-changelog-cli": "^1.3.1",
    "eslint": "^3.18.0",
    "eslint-config-unstandard": "^1.4.2",
    "istanbul": "^0.4.5",
    "mocha": "^2.4.5",
    "sinon": "^2.0.0",
    "sinon-chai": "^2.8.0",
    "supertest": "^2.0.1",
    "supertest-as-promised": "^4.0.2",
    "yuidoc-lucid-theme": "^0.1.2",
    "yuidocjs": "^0.10.2"
  },
  "eslintConfig": {
    "extends": "unstandard"
  }
}
